{
  "name": "worshipnote",
  "version": "1.0.0",
  "description": "찬양 악보 관리 앱",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"sleep 10 && electron .\" --kill-others-on-fail --prefix-colors blue,green",
    "dev": "npm run electron-dev",
    "electron-only": "npx electron . --disable-gpu --no-sandbox --disable-web-security --disable-features=VizDisplayCompositor",
    "electron-safe": "npx electron . --disable-gpu --no-sandbox --disable-web-security --disable-features=VizDisplayCompositor --disable-dev-shm-usage --disable-extensions --disable-background-timer-throttling --disable-backgrounding-occluded-windows --disable-renderer-backgrounding",
    "electron-pack": "electron-builder",
    "preelectron-pack": "npm run build"
  },
  "build": {
    "appId": "com.worshipnote.app",
    "productName": "WorshipNote",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/electron.js",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.music"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "date-fns": "^2.29.3",
    "electron": "^38.1.1",
    "electron-is-dev": "^2.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.263.1",
    "pdfjs-dist": "^5.4.149",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron-builder": "^24.6.4",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
